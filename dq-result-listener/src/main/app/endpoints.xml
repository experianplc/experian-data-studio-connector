    <flow name="dq2dgcSynchronousEntryPoint" processingStrategy="synchronous">
        <vm:inbound-endpoint exchange-pattern="request-response"  doc:name="dq2dgc_dqresult_synchronous" connector-ref="domain-shared-vm-connector" path="${vmqueuepath}"/>
        <logger message="Stating flow DQ Result to DGC synchronously" level="DEBUG" doc:name="Stating"/>
        <flow-ref name="main" doc:name="main"/>
        <logger message="Finished flow DQ Result to DGC" level="DEBUG" doc:name="Finished"/>
        <exception-strategy ref="defaultChoiceExceptionStrategy" doc:name="Default exception strategy"/>
    </flow>



